    <profiles>
        <profile>
            <id>docker</id><!--serve para criarmos uma imagem direto com maven -->
            <build>         <!-- aula 24.11. Construindo a imagem Docker pelo Maven-->
                <plugins>
                    <plugin>
                      <groupId>com.spotify</groupId>
                      <artifactId>dockerfile-maven-plugin</artifactId>
                      <version>1.4.13</version>
                      <executions>
                        <execution>
                          <id>default</id>
                          <goals>
                            <goal>build</goal>
                            <goal>push</goal>
                          </goals>
                        </execution>
                      </executions>
                      <configuration>
                        <repository>algafood-api</repository>
                            <buildArgs>
                          <JAR_FILE>${project.build.finalName}.jar</JAR_FILE><!--usamos a tag JAR_FILE  no arquivo Dockerfile. serve para reconhece a versao do projeto toda vez q fazmos um build vai alterar automaticamente-->
                        </buildArgs>
                      </configuration>
                    </plugin>
                </plugins>
            </build>
        </profile>
    </profiles>
